<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.dialogs.eventEdit.EventEditController">
   <bottom>
      <ButtonBar BorderPane.alignment="CENTER">
        <buttons>
          <Button fx:id="confirm" defaultButton="true" mnemonicParsing="false" text="Confirm" />
        </buttons>
         <BorderPane.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </BorderPane.margin>
      </ButtonBar>
   </bottom>
   <top>
      <ToolBar maxWidth="1.7976931348623157E308" BorderPane.alignment="CENTER">
        <items>
          <Button fx:id="messageAllAttendee" mnemonicParsing="false" text="Message All Attendee" />
            <Button fx:id="cancel" cancelButton="true" mnemonicParsing="false" text="Cancel Event" />
        </items>
      </ToolBar>
   </top>
   <center>
      <BorderPane fx:id="detailPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" BorderPane.alignment="CENTER">
         <bottom>
            <TextArea fx:id="respond" prefHeight="100.0" BorderPane.alignment="CENTER">
               <BorderPane.margin>
                  <Insets left="5.0" right="5.0" top="5.0" />
               </BorderPane.margin></TextArea>
         </bottom>
      </BorderPane>
   </center>
</BorderPane>
